{
  "name": "agendamento-quadras-api",
  "version": "1.0.0",
  "description": "API de autenticação para Agendamento de Quadras",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "docker:dev": "docker-compose up",
    "docker:build": "docker build -t agendamento-quadras-api .",
    "seed": "node ./scripts/seed.js",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "migrate:mysql": "node ./scripts/migrate-mysql.js",
    "seed:rbac": "node ./scripts/seed-rbac.js",
    "verify:migration": "node ./scripts/verify-mysql-migration.js"
  },
  "keywords": [
    "auth",
    "api",
    "nodejs",
    "express",
    "mysql",
    "sequelize",
    "quadras",
    "agendamento"
  ],
  "author": "Agendamento de Quadras",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.5",
    "nodemailer": "^6.10.0",
    "node-cache": "^5.1.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.3",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "validator": "^13.11.0",
    "winston": "^3.17.0",
    "xss": "^1.0.15",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ]
  }
}